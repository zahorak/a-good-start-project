/**
 * Helper function wrapping around JsonRpc.execute.
 */
var execute = function(method, args, callback) {
	JsonRpc.execute(
		function(msg) { messenger.sendMessage(msg); },
		method,
		args,
		callback
	);
};

/**
 * Posts a comment.
 */
var isCommentInvalid = function() {
    return $('form-message').value == comment.placeholder.textarea;
}

var validateComment = function(id) {
	if (isCommentInvalid()) {
		execute('validateComment.onFailure', [{ 'id': id, message: "Please enter a comment to post."}]);
		return false;
	}

	execute('validateComment.onSuccess', [{'id': id}]);
	return true;
}

var postComment = function(author_name, author_email, author_url, authenticate, sharing_services, subscribe, captcha) {
    if (isCommentInvalid()) {
        execute('postComment.onFailure', [{message: "Please enter a comment to post."}]);
        return;
    }

	var postData = {
		message: $('form-message').value
	};
	if (author_name) {
		postData.author_name = author_name;
	}
	if (author_email) {
		postData.author_email = author_email;
	}
	if (author_url) {
		postData.author_url = author_url;
	}
	if (subscribe) {
		postData.subscribe = subscribe;
	}
	if (authenticate) {
		postData.auth_choice = authenticate.auth_choice;
		postData.auth_username = authenticate.username;
		postData.auth_password = authenticate.password;
		postData.auth_email = authenticate.email;
	}

	if (sharing_services) {
		for (var code in sharing_services) {
			if (sharing_services.hasOwnProperty(code) === true) {
				postData[code] = sharing_services[code];
			}
		}
	}
	if (state.parent_post_id) {
		postData.parent_post_id = state.parent_post_id;
	}
	if (state.depth || state.depth === 0) {
		postData.parent_post_depth = state.depth;
	}

	if (captcha) {
	    postData.recaptcha_challenge_field = captcha.recaptcha_challenge_field;
	    postData.recaptcha_response_field = captcha.recaptcha_response_field;
	}

	$AJR('http://diminishing.disqus.com/thread/extending_formtastic_with_a_sprinkle_of_jquery_diminishingorg_44/post_create/', {
		success: function(response) {
			execute('postComment.onSuccess', [response]);

			// Reset values to defaults.
			$('form-message').value = comment.placeholder.textarea;
			$('form-message').className += ' placeholder-grey';

			if (authenticate) {
				execute('reload', []);
			}
		},
		failure: function(code, response) {
			try {
				response = eval('(' + response + ')');
				response.request = {
				    post: postData
				};
			} catch (ex) {
				response = {message: "There was an error with your request."};
			}
			execute('postComment.onFailure', [response]);
		}
	}, postData);
};

